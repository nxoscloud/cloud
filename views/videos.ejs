<!DOCTYPE html>
<html>
<head>
    <title>Videos</title>
</head>
<body>
    <h1>Videos</h1>
    <% if (videos.length > 0) { %>
        <% videos.forEach(video => { %>
            <div>
                <video width="320" height="240" controls>
                    <source src="/videos/<%= video %>" type="video/mp4">
                    Your browser does not support the video tag.
                </video>
                <p><%= video %></p>
            </div>
        <% }) %>
    <% } else { %>
        <p>No videos found</p>
    <% } %>
</body>
</html>
